            @if (ViewBag.type == 1)
            {
                <input type="number" class="number0" style="width:50px;border: 0;border-bottom: 1px solid #000;text-align: center;border-radius: 0;" name="DeliveryDayIn" value="@Model.DeliveryDayIn" onmousewheel="return false;" />
