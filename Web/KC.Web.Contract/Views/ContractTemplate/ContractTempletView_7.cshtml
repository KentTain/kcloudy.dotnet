    <p style="text-align:center;line-height:120%"><strong><span style="font-size: 19px;line-height:120%;font-family: 黑体">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:120%"><strong><span style="font-size: 16px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:120%"><strong><span style="font-size: 16px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="text-indent:169px;line-height:120%">
        <strong>
            <span style="font-size:19px;line-height:120%;font-family:黑体;color:black">
                出租人：
                <span style="text-decoration:underline;">
                    @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.VendorName))
                    {
                        @Html.Raw(ContractTemplateOrderDTO.VendorName);
                    }
                    else
                    {
                        @Html.Raw("___________________________________");
                    }
                </span>
            </span>
        </strong>
    </p>
    <p style="text-indent:169px;line-height:120%">
        <strong>
            <span style="font-size:19px;line-height:120%;font-family:黑体;color:black">
                承租人：
                <span style="text-decoration:underline;">
                    @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.CustomerName))
                    {
                        @Html.Raw(ContractTemplateOrderDTO.CustomerName);
                    }
                    else
                    {
                        @Html.Raw("___________________________________");
                    }
                </span>
            </span>
        </strong>
    </p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size: 24px;line-height:150%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="">
        <strong><span style="font-size:19px;font-family:仿宋">出 租 人：</span></strong>
        <span style="font-size:19px;">
            @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.VendorName))
            {
                <span style="text-decoration:underline;">
                    @Html.Raw(ContractTemplateOrderDTO.VendorName)
                </span>
            }
            else
            {
                @Html.Raw("______________________")
            }
            （以下简称<strong>“甲方”</strong>）
        </span>
    </p>
    <p style="">
        <strong><span style="font-size:19px;font-family:仿宋">公司地址：</span></strong>
        <span style="font-size:19px;">
            @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.VendorAddress1))
            {
                <span style="text-decoration:underline;">
                    @Html.Raw(ContractTemplateOrderDTO.VendorAddress1)
                </span>
            }
            else
            {
                @Html.Raw("________________________________")
            }
        </span>
    </p>
    <p style="">
        <strong><span style="font-size:19px;font-family:仿宋">法人代表：</span></strong>
        <span style="font-size:19px;">
            @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.JiaLegal))
            {
                <span style="text-decoration:underline;">
                    @Html.Raw(ContractTemplateOrderDTO.JiaLegal)
                </span>
            }
            else
            {
                @Html.Raw("________________________________")
            }
        </span>
    </p>
    <p style="line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="">
        <strong><span style="font-size:19px;font-family:仿宋">承 租 人：</span></strong>
        <span style="font-size:19px;">
            @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.CustomerName))
            {
                <span style="text-decoration:underline;">
                    @Html.Raw(ContractTemplateOrderDTO.CustomerName)
                </span>
            }
            else
            {
                @Html.Raw("______________________")
            }
