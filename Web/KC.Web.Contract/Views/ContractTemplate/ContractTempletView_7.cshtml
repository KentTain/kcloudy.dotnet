
@model KC.Service.DTO.Contract.ContractTemplateValueDTO
@using KC.Service.DTO.Contract
@{

    Layout = "~/Views/Shared/_Layout_ContractTemplateView.cshtml";
    var ContractTemplateOrderDTO = ViewBag.ContractTemplateOrderDTO as ContractTemplateOrderDTO;
    if (ContractTemplateOrderDTO == null)
    {
        ContractTemplateOrderDTO = new ContractTemplateOrderDTO();
    }
}


<input type="hidden" value="@ViewBag.Id" name="Id" />
<div>
    <p style="text-align:right;padding-right: 100pt;">
        <span style="font-size:16px;line-height:120%;font-family: 楷体;color:black">合同编号：</span>
        <span style="text-decoration:underline;">
            <span style="color:gray">
                @if (ViewBag.contractNo != null)
                {
                    @Html.Raw(ViewBag.contractNo);
                }
                else
                {
                    @Html.Raw("_______________________________");
                }
            </span>
        </span>
    </p>
    <p style="text-align:center;line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:120%"><strong><span style="font-size: 48px;line-height:120%;font-family: 黑体">&nbsp;</span></strong></p>
    <p style="line-height:120%"><strong><span style="font-size: 24px;line-height:120%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
