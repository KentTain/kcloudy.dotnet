                    <td style="border-bottom:#808080 solid 0.75pt; border-right:#808080 solid 0.75pt; border-top:#808080 solid 0.75pt;  padding-left:5.03pt; padding-left:5.03pt; padding-right:5.03pt; vertical-align:middle; width:100pt"><p style="font-size:10.5pt; line-height:150%; margin:0pt; text-align:center"><span style="font-family:宋体; font-size:11pt; font-weight:bold">使用时间</span></p></td>
                </tr>
                @{ var it = 0; var isbl = false;}
                @if (systemAppreciationDTO != null && systemAppreciationDTO.Any())
                {
                    foreach (var item in systemAppreciationDTO)
                    {
                        if (item != null && item.Id < 13)
                        {
                            it++;
                            <tr style="height: 30px;">
                                <td style="border-bottom:#808080 solid 0.75pt; border-left:#808080 solid 0.75pt; border-right:#808080 solid 0.75pt; padding-left:5.03pt; padding-left:5.03pt; padding-right:5.03pt; vertical-align:middle;width:100pt"><p style="font-size:10.5pt; line-height:150%; margin:0pt"><span style="font-family:宋体; font-size:10.5pt">@item.AppreciationName</span></p></td>
                                <td style="border-bottom:#808080 solid 0.75pt; border-right:#808080 solid 0.75pt; padding-left:5.03pt; padding-left:5.03pt; padding-right:5.03pt; vertical-align:middle; width:150pt;text-align:left"><p style="font-size:10.5pt; line-height:150%; margin:0pt"><span style="font-family:宋体; font-size:10.5pt">@item.Description</span></p></td>
