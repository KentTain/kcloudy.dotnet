                    @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.VendorContactPerson))
                    {
                        @Html.Raw(ContractTemplateOrderDTO.VendorContactPerson);
                    }
                    else
                    {
                        @Html.Raw("___________________________________");
                    }
                </span>
            </span>
        </strong>
    </p>
    <p style="line-height: 150%">
        <strong>
            <span style="font-family:黑体;color:gray">
                联系方式：<span style="text-decoration:underline;" class="VendorContactPhone">
                    @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.VendorContactPhone))
                    {
                        @Html.Raw(ContractTemplateOrderDTO.VendorContactPhone);
                    }
                    else
                    {
                        @Html.Raw("___________________________________");
