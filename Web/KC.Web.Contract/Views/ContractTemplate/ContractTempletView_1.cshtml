        {

            <input type="number" class="number0"  style="width:60px;border: 0;border-bottom: 1px solid #000;text-align: center;border-radius: 0;" name="Day" onmousewheel="return false;" value="@Model.Day" />
        }
        else
        {
            <span style="font-size:19px;font-family:仿宋;text-decoration:underline;line-height: 150%">
                @Html.Raw(Model.Day)
            </span>
        }
        <span style="font-size:19px;font-family:仿宋;color:gray;line-height: 150%">日之内。</span>
    </p>
    <p style="text-indent: 56px;line-height: 150%;font-size:19px;">
        <span style="font-size:19px;font-family:仿宋;color:gray;line-height: 150%">2.</span><span style="font-size:19px;font-family:仿宋;color:gray;line-height: 150%">
            收货地点为：
            <span style="text-decoration:underline;" class="ShipToAddress1">
                @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.ShipToAddress1))
                {
                    @Html.Raw(ContractTemplateOrderDTO.ShipToAddress1);
                }
                else
                {
                    @Html.Raw("___________________________________");
                }
            </span>
        </span>
    </p>
    <p style="text-indent: 56px;line-height: 150%;font-size:19px;">
        <span style="font-size:19px;font-family:仿宋;color:gray;line-height: 150%">3.</span><span style="font-size:19px;font-family:仿宋;color:gray;line-height: 150%">
            收货联系人：
            <span style="text-decoration:underline;" class="ShipToContactPerson">
                @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.ShipToContactPerson))
                {
                    @Html.Raw(ContractTemplateOrderDTO.ShipToContactPerson);
                }
                else
                {
                    @Html.Raw("___________________________________");
                }
            </span>
        </span>
