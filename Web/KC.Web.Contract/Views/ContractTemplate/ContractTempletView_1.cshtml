    <p style="text-indent: 56px;line-height: 150%;font-size:19px;">
        <span style="font-size:19px;font-family:仿宋;color:gray;line-height: 150%">4.</span><span style="font-size:19px;font-family:仿宋;color:gray;line-height: 150%">
            收货联系电话：
            <span style="text-decoration:underline;" class="ShipToContactPhone">
                @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.ShipToContactPhone))
                {
                    @Html.Raw(ContractTemplateOrderDTO.ShipToContactPhone);
                }
