            @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.CustomerName))
            {
                @Html.Raw(System.DateTime.UtcNow.AddHours(8).ToString("yyyy年 MM月 dd日"));
            }
            else
            {
                @Html.Raw(" 年 月 日");
            }
