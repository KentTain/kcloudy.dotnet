
@model KC.Service.DTO.Contract.ContractTemplateValueDTO
@using KC.Service.DTO.Contract
@{

    Layout = "~/Views/Shared/_Layout_ContractTemplateView.cshtml";
    var ContractTemplateOrderDTO = ViewBag.ContractTemplateOrderDTO as ContractTemplateOrderDTO;
    if (ContractTemplateOrderDTO == null)
    {
        ContractTemplateOrderDTO = new ContractTemplateOrderDTO();
    }
}


<input type="hidden" value="@ViewBag.Id" name="Id" />
<div>
    <p style="text-align:right;padding-right: 100pt;">
        <span style="font-size:16px;line-height:150%;font-family: 楷体;color:black">合同编号：</span>
        <span style="text-decoration:underline;">
            <span style="color:gray">
                @if (ViewBag.contractNo != null)
                {
                    @Html.Raw(ViewBag.contractNo);
                }
                else
                {
                    @Html.Raw("_______________________________");
                }
            </span>
        </span>
    </p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size: 24px;line-height: 150%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:150%"><strong><span style="font-size: 24px;line-height: 150%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size: 24px;line-height: 150%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size: 24px;line-height: 150%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size: 48px;line-height: 150%;font-family: 黑体">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size:29px;line-height: 150%;font-family:黑体;color:black">买卖合同</span></strong></p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size:19px;line-height: 150%;font-family:楷体;color:black">（融资租赁业务）</span></strong></p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size: 19px;line-height: 150%;font-family: 黑体">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size: 16px;line-height: 150%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size: 16px;line-height: 150%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:150%"><strong><span style="font-size: 24px;line-height: 150%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:150%"><strong><span style="font-size: 24px;line-height: 150%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height: 150%"><strong><span style="font-size:19px;line-height:150%;font-family:宋体;color:gray">&nbsp;</span></strong></p>
    <p style="text-indent:169px;line-height:150%">
        <strong>
            <span style="font-size:19px;line-height:150%;font-family:黑体;color:black">
                买方：
                <span style="text-decoration:underline;">
                    @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.CustomerName))
                    {
                        @Html.Raw(ContractTemplateOrderDTO.CustomerName);
                    }
                    else
                    {
                        @Html.Raw("___________________________________");
                    }
                </span>
            </span>
        </strong>
    </p>
    <p style="text-indent:169px;line-height:150%">
        <strong>
            <span style="font-size:19px;line-height:150%;font-family:黑体;color:black">
                卖方：
                <span style="text-decoration:underline;">
                    @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.VendorName))
                    {
                        @Html.Raw(ContractTemplateOrderDTO.VendorName);
                    }
                    else
                    {
                        @Html.Raw("___________________________________");
                    }
                </span>
            </span>
        </strong>
    </p>
    <p style="text-indent:169px;line-height:150%">
        <strong>
            <span style="font-size:19px;line-height:150%;font-family:黑体;color:black">
                承租人：
                <span style="text-decoration:underline;">
                    @if (!string.IsNullOrEmpty(ContractTemplateOrderDTO.Lessee))
                    {
                        @Html.Raw(ContractTemplateOrderDTO.Lessee);
                    }
                    else
                    {
                        @Html.Raw("___________________________________");
                    }
                </span>
            </span>
        </strong>
    </p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size: 24px;line-height: 150%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="text-align:center;line-height:150%"><strong><span style="font-size: 24px;line-height: 150%;font-family: 仿宋_GB2312">&nbsp;</span></strong></p>
    <p style="line-height:150%">
        <strong><span style="font-size:19px;line-height:150%;font-family:黑体">买方：</span></strong>
