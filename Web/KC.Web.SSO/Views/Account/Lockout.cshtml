@model KC.Web.SSO.AccountViewModels.LogoutViewModel

@{
    ViewData["Title"] = "锁定账户";
    ViewData["signed-out"] = true;
    Layout = "~/Views/Shared/_HomeLayout.cshtml";
}
<div class="main">
    <div class="text-center logout-page" style="margin-top:120px">
        <div class="page-header">
            <h1>账户已被冻结</h1>
        </div>

        <div class="row">
            <div class="col-sm-6">

                <form asp-action="Logout">
                    <input type="hidden" name="logoutId" value="@Html.ValueFor(m=>m.LogoutId)" />
                    <br />
                    <div class="form-group">
                        <button class="layui-btn layui-btn-normal ">确定</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
