@using KC.Enums.Portal
<style>
    .img1:hover .Triangle2, .img2:hover .Triangle2, .img3:hover .Triangle2, .img4:hover .Triangle2, .img5:hover .Triangle2 {
        display: block;
    }

    .tandivdiv1_1, .tandivdiv2_1, .tandivdiv3_1, .tandivdiv4_1, .tandivdiv5_1 {
        display: none;
        background-size: 100%;
        width: 85px;
        height: 30px;
        line-height: 32px;
        color: #FFFFFF;
        position: absolute;
        right: 49px;
        border-radius: 3px;
        text-align: center;
        background-color: #FF5600;
        top: 7px;
    }

        .tandivdiv1_1 img, .tandivdiv2_1 img, .tandivdiv3_1 img, .tandivdiv4_1 img, .tandivdiv5_1 img {
            width: 110px;
            height: 110px;
            right: -13px;
            position: absolute;
            margin-top: 12px;
            background: white;
        }

    .img1:hover .tandivdiv1_1, .img2:hover .tandivdiv2_1, .img3:hover .tandivdiv3_1, .img4:hover .tandivdiv4_1, .img5:hover .tandivdiv5_1 {
        display: block;
    }

    .img3_1 {
        background: none;
    }

    .img2_1 {
        background: #FF5600;
        background-position: -2px -10px;
    }
</style>
<body>

    <div class="tandiv">
        <div class="juzhong_wrap">
            <div class="tandivdiv img1" onclick="addFavorite('@ViewBag.TenantName',@((int)FavoriteType.Company))">
                <div class="tandivdiv1_1 img1_1">收藏店铺</div>
                <div class="Triangle2"></div>
            </div>
            <div class="tandivdiv img2">
                <div class="tandivdiv2_1 img2_1">QQ咨询</div>
                <div class="Triangle2"></div>
            </div>
            <div class="tandivdiv img3">
                <div class="tandivdiv3_1 img3_1"><img src="/Content/images/home_1/footer-qr.png" width="153" height="153" style=" right: 0px; top: -50px;" /></div>
            </div>
            <div class="tandivdiv supplier_apply" onclick="applyProvider()">
                <div class="tandivdiv2_1 img4_1">申请成为供应商</div>
                <div class="Triangle2"></div>
            </div>
            <div class="tandivdiv supplier_add" onclick="addProvider()">
                <div class="tandivdiv2_1 img5_1">添加为我的供应商</div>
                <div class="Triangle2"></div>
            </div>
        </div>
        <a href="#">
            <div class="tandivdiv img4">
                <div class="tandivdiv4_1 img4_1">返回顶部</div>
                <div class="Triangle2"></div>
            </div>
        </a>

    </div>
</body>
